{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "MinimalLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "Kafka": {
    "ProducerConfig": {
      "BootstrapServers": "localhost:9092",
      "MessageTimeoutMs": "10000"

    },
    "ConsumerConfig": {
      "BootstrapServers": "localhost:9092"
    }
  }
}
